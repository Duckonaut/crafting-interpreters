fn fib(n) {
    if (n <= 1) { return n; }
    return fib(n - 2) + fib(n - 1);
}

for (var mut i = 0; i < 40; i = i + 1) {
    if i == 20 {
        println("ok 20 is a lot of numbers lets wrap it up");
        break;
    }
    else if i < 10 {
        print("Fibonacci number " + show(i) + ": ");
    }
    else if i == 10 {
        print("ok you get it, ");
    }
    
    println(fib(i));
}
